(defn keywordize
  [m]
  (into {}
        (for [[k v] m]
             (do
               (println k (type v) (map? v))
               [(keyword k) (if (vector? v)
                                ()
                                (if ()
                                    ()
                                    ()))]))))
(keywordize m)
(defn keywordize
  [m]
  (into {}
        (for [[k v] m]
             (do
               (println k (type v) (map? v))
               [(keyword k) 
                (cond
                  (vector? v) (vec (map keywordize v)
                  (map? v) (keywordize v)
                  :else v))
                #_(if (vector? v)
                                (vec (map keywordize v))
                                (if ()
                                    ()
                                    ()))]))))
(keywordize m)
