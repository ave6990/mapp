(defmacro make-get-page
  [title table-id fields-settings]
  `(let [req ~(symbol "req")]
     (defn ~(symbol (str "get-" talbe-id "-page"))
       [~req]
       (get-page
         ~title
         ~talbe-id
         ~req
         ~(symbol (str "midb/get-" table-id))
         ~fields-settings))))
