(defn keywordize
  [m]
  (into {}
        (for [[k v] m]
             [(keyword k) v])))
(def m
  {"id" 1
   "table" "ver"
   "data" []
   "obj" {"a" 1 "b" 2}}})
(defn keywordize
  [m]
  (into {}
        (for [[k v] m]
             (do
               (println k (type v))
               [(keyword k) (if (vector? v)
                                ()
                                ())]))))
