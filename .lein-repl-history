(defn keywordize
  [m]
  (into {}
        (for [[k v] m]
             (do
               (println k (type v) (map? v))
               [(keyword k) 
                (cond
                  (vector? v)
                    (vec
                      (map (fn [vl]
                               (if (map? vl)
                                   (keywordize vl)
                                   vl))
                           v))
                  (map? v) (keywordize v)
                  :else v)]))))
(keywordize m)
